@model TransCanada.Models.TpaLabservice

@{
    ViewBag.Title = "CreateTpalabService";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CreateTpalabService</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>TpaLabservice</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
       

            @Html.HiddenFor(model => model.TPALab_Id, htmlAttributes: new { @class = "control-label col-md-2" })
           

        <div class="form-group">
            @Html.LabelFor(model => model.Service_Grp_Name, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Service_Grp_Name, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Service_Grp_Name, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "TpaLabService", new { id = Model.TPALab_Id })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
